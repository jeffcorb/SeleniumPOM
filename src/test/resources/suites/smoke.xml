<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" verbose="2" time-out="60000">
    <groups>
        <run>
            <include name="smoke"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="listeners.TestListeners"/>
        <listener class-name="listeners.SuiteListeners"/>
    </listeners>
    
    <test name="saucedemo">
        <packages>
            <package name="saucedemo"/>
        </packages>
    </test>
</suite>